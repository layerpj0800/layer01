# Database connection string; include credentials and host
DATABASE_URL=postgresql://user:pass@localhost:5432/db

# Secret key for JWT signing; replace with a strong value
JWT_SECRET=change_me

# Allowed origins for CORS, comma separated
CORS_ORIGINS=https://frontend.myapp.com

# S3 bucket name for media uploads
S3_BUCKET=your-bucket

# Iamport API key for payment verification
IAMPORT_API_KEY=

# Cloudflare Stream credentials
CF_ACCOUNT_ID=
CF_API_TOKEN=

# Firebase Cloud Messaging server key
FCM_SERVER_KEY=

# SendGrid API key for email delivery
SENDGRID_API_KEY=
